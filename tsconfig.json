{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "app-type/*": [
        "types/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "src/**/*.ts"
  ]
}